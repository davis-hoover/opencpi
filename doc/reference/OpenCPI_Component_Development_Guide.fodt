   <text:p text:style-name="P306">{<text:line-break/> <text:s/>&quot;project&quot;: {<text:line-break/> <text:s text:c="3"/>&quot;directory&quot;: <text:span text:style-name="T985">&lt;String – Project directory&gt;</text:span>,<text:line-break/> <text:s text:c="3"/>&quot;package&quot;: <text:span text:style-name="T985">&lt;String – Project package-id&gt;</text:span>,<text:line-break/> <text:s text:c="3"/>&quot;dependencies&quot;: <text:span text:style-name="T985">&lt;Array of strings – package-ids of projects to<text:line-break/> <text:s text:c="39"/>depend on&gt;</text:span>,<text:line-break/> <text:s text:c="3"/>&quot;libraries&quot;: {<text:line-break/> <text:s text:c="5"/>&lt;String- package id of library&gt;: &lt;String- directory of library&gt;,<text:line-break/> <text:s text:c="5"/><text:span text:style-name="T81">… more libraries</text:span><text:span text:style-name="T307"><text:line-break/></text:span> <text:s text:c="3"/>},<text:line-break/> <text:s text:c="3"/>&quot;components&quot;: {<text:line-break/> <text:s text:c="5"/>&lt;String- <text:span text:style-name="T986">name of component</text:span>&gt;: &lt;String - directory of library&gt;,<text:line-break/> <text:s text:c="5"/><text:span text:style-name="T81">… more components</text:span><text:span text:style-name="T307"><text:line-break/></text:span> <text:s text:c="3"/>},<text:line-break/> <text:s text:c="3"/>&quot;platforms&quot;: {<text:line-break/> <text:s text:c="5"/>&quot;rcc&quot;: {<text:line-break/> <text:s text:c="7"/>&lt;String- <text:span text:style-name="T986">name of rcc platform</text:span>&gt;: &lt;String - directory of <text:span text:style-name="T986">rcc<text:line-break/> <text:s text:c="40"/>platform</text:span>&gt;,<text:line-break/> <text:s text:c="7"/><text:span text:style-name="T81">… more rcc platforms</text:span><text:line-break/> <text:s text:c="5"/>},<text:line-break/> <text:s text:c="5"/>&quot;hdl&quot;: {<text:line-break/> <text:s text:c="7"/>&lt;String- <text:span text:style-name="T986">name of rcc platform</text:span>&gt;: &lt;String - directory of <text:span text:style-name="T986">hdl<text:line-break/> <text:s text:c="40"/>platforms</text:span>&gt;,<text:line-break/> <text:s text:c="7"/><text:span text:style-name="T81">… more hdl platforms</text:span><text:span text:style-name="T307"><text:line-break/></text:span> <text:s text:c="5"/>}<text:line-break/> <text:s text:c="3"/>}<text:line-break/> <text:s/>}<text:line-break/>}</text:p>
