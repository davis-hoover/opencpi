   <text:p text:style-name="Glossary_20_Definition"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T107">A </text:span></text:span><text:span text:style-name="FirstEmphasis">Zero-Length Message (ZLM)</text:span><text:span text:style-name="Emphasis"><text:span text:style-name="T95"> </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T95">is a d</text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T110">ata payload with no </text:span></text:span><text:a xlink:type="simple" xlink:href="#operation argument" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T110">operation arguments</text:span></text:span></text:a><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T110"> present when a </text:span></text:span><text:a xlink:type="simple" xlink:href="#OpenCPI Protocol Specification" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T110">protocol specification</text:span></text:span></text:a><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T110"> </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T94">specifies</text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T110"> such an </text:span></text:span><text:a xlink:type="simple" xlink:href="#operation code" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T110">operation code</text:span></text:span></text:a><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T110"> with no data fields.</text:span></text:span></text:p>
   <text:h text:style-name="Heading_20_2" text:outline-level="2">Industry Terminol<text:span text:style-name="T70">o</text:span>gy</text:h>
   <text:p text:style-name="P65">This section provides definitions for industry-wide terms <text:span text:style-name="T85">relating to OpenCPI.</text:span></text:p>
   <text:p text:style-name="Glossary_20_Term"><text:bookmark text:name="Advanced eXtensible Interface"/><text:span text:style-name="Default_20_Paragraph_20_Font">Advanced eXtensible Interface (AXI)</text:span></text:p>
   <text:p text:style-name="Glossary_20_Definition"><text:span text:style-name="FirstEmphasis">Advanced eXtensible Interface (AXI)</text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T107"> is </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T95">an i</text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T102">ndustry-standard bus used by ARM processors.</text:span></text:span></text:p>
   <text:p text:style-name="Glossary_20_Term"><text:bookmark text:name="Advanced RISC Machine"/><text:span text:style-name="Default_20_Paragraph_20_Font">Advanced RISC Machine (ARM)</text:span></text:p>
   <text:p text:style-name="Glossary_20_Definition"><text:span text:style-name="FirstEmphasis">Advanced RISC Machine (ARM)</text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T107"> is </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T95">a</text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T62"> </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T85">widely-used </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T62">processor architecture </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T85">originally </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T62">based on a 32-bit reduced instruction set (RISC) computer.</text:span></text:span></text:p>
   <text:p text:style-name="Glossary_20_Term"><text:span text:style-name="Default_20_Paragraph_20_Font">ARM</text:span></text:p>
   <text:p text:style-name="Glossary_20_Definition"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T107">See </text:span></text:span><text:a xlink:type="simple" xlink:href="#Advanced RISC Machine" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="FirstEmphasis">Advanced RISC Machine</text:span></text:a><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T107">.</text:span></text:span></text:p>
