ple" xlink:href="#container" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T298">co</text:span></text:span></text:a><text:a xlink:type="simple" xlink:href="#container" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T240">ntainers</text:span></text:span></text:a><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T240">.</text:span></text:span></text:p>
