    <text:p text:style-name="Glossary_20_Definition"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T305">A </text:span></text:span><text:span text:style-name="FirstEmphasis">device proxy worker</text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T305"> is </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T304">a </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T258">software</text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T305"> </text:span></text:span><text:a xlink:type="simple" xlink:href="#worker" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T305">worker</text:span></text:span></text:a><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T305"> (RCC/C++) that is specifically paired with </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T258">one or more</text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T276"> </text:span></text:span><text:a xlink:type="simple" xlink:href="#HDL device worker" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T276">HDL</text:span></text:span></text:a><text:a xlink:type="simple" xlink:href="#HDL device worker" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T305"> device worke</text:span></text:span></text:a><text:a xlink:type="simple" xlink:href=