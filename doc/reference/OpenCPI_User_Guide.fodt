    <text:p text:style-name="Glossary_20_Definition"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T305">See </text:span></text:span><text:a xlink:type="simple" xlink:href="#component unit test" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="FirstEmphasis">component unit </text:span></text:a><text:a xlink:type="simple" xlink:href="#component unit test" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="FirstEmphasis"><text:span text:style-name="T304">test suite</text:span></text:span></text:a><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T272">.</text:span></text:span></text:p>
