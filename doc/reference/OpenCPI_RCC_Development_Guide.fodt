    <text:p text:style-name="Glossary_20_Definition"><text:span text:style-name="FirstEmphasis">Advanced RISC Machine (ARM)</text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T837"> is </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T836">a</text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T748"> </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T792">widely-used </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T748">processor architecture </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T792">originally </text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T748">based on a 32-bit reduced instruction set (RISC) computer.</text:span></text:span></text:p>
    <text:p text:style-name="Glossary_20_Term"><text:span text:style-name="Default_20_Paragraph_20_Font">ARM</text:span></text:p>
    <text:p text:style-name="Glossary_20_Definition"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T837">See </text:span></text:span><text:a xlink:type="simple" xlink:href="#Advanced RISC Machine" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="FirstEmphasis">Advanced RISC Machine</text:span></text:a><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T837">.</text:span></text:span></text:p>
