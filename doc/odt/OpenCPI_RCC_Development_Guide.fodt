    <table:table-column table:style-name="Table8.A"/>
    <table:table-column table:style-name="Table8.B"/>
    <table:table-row table:style-name="Table8.1">
     <table:table-cell table:style-name="Table8.A1" office:value-type="string">
      <text:p text:style-name="P104"><text:span text:style-name="T542">Method</text:span> Name <text:span text:style-name="T543">and Arguments</text:span></text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table8.A1" office:value-type="string">
      <text:p text:style-name="P109"><text:span text:style-name="T7">Member</text:span><text:line-break/><text:span text:style-name="T7">Description</text:span></text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row table:style-name="Table8.1">
     <table:table-cell table:style-name="Table8.A2" office:value-type="string">
      <text:p text:style-name="P421">disableTimeout()</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table8.A2" office:value-type="string">
      <text:p text:style-name="P109"><text:span text:style-name="T543">Disable the timeout aspect of the run conditions. <text:s/>The timeout time value (</text:span><text:span text:style-name="Code">usecs</text:span><text:span text:style-name="T543">) is unchanged.</text:span></text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row table:style-name="Table8.1">
     <table:table-cell table:style-name="Table8.A3" office:value-type="string">
      <text:p text:style-name="P421">enableTimeout()</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table8.A3" office:value-type="string">
      <text:p text:style-name="P109"><text:span text:style-name="T543">Enable the timeout aspect of the run condition, using the previously specified </text:span><text:span text:style-name="Code">usecs</text:span><text:span text:style-name="T543"> value.</text:span></text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row table:style-name="Table8.1">
     <table:table-cell table:style-name="Table8.A3" office:value-type="string">
      <text:p text:style-name="P421">enableTimeout<text:line-break/>(uint32_t usecs)</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table8.A3" office:value-type="string">
      <text:p text:style-name="P112"><text:span text:style-name="T543">Enable the timeout aspect of the run condition, supplying a new </text:span><text:span text:style-name="Code">usecs</text:span><text:span text:style-name="T543"> value, which will be stored in the object.</text:span></text:p>
     </table:table-cell>
    </table:table-row>
