<!-- FPGA_GPIO1/2/3 = OUT/OUT/OUT -->
<Card Type="matchstiq_z1_gpio_slot">
  <Device Worker="gpo" Name="gpo">
    <Property Name="NUM_OUTPUTS" Value="3"/>
    <Signal Name="gpo_pin(0)" Slot="FPGA_GPIO1"/>     <!-- Pin U6 -->
    <Signal Name="gpo_pin(1)" Slot="FPGA_GPIO2"/>     <!-- Pin Y10 -->
    <Signal Name="gpo_pin(2)" Slot="FPGA_GPIO3"/>     <!-- Pin Y6 -->
  </Device>
</Card>
