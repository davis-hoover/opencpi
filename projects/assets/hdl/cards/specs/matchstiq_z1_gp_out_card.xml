<!-- FPGA_GPIO1/2/3 = OUT/OUT/OUT -->
<Card Type="matchstiq_z1_gpio_slot">
  <Device Worker="matchstiq_z1_gp_out" Name="matchstiq_z1_gp_out">
    <Property Name="simulation_p" Value="false"/>
    <Signal Name="gpio1" Slot="FPGA_GPIO1"/>     <!-- Pin U6 -->
    <Signal Name="gpio2" Slot="FPGA_GPIO2"/>     <!-- Pin Y10 -->
    <Signal Name="gpio3" Slot="FPGA_GPIO3"/>     <!-- Pin Y6 -->
  </Device>
</Card>
