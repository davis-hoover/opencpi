<!--
  The dac_clk_freq_hz property is only used by the emulator worker,
  but currently, emulators can only be parameterized by properties
  of the worker they are emulating (Issue #131).
-->
<build>
  <configuration id='0'>
    <parameter name='dac_clk_freq_hz' value='1e6'/>
  </configuration>
  <configuration id='1'>
    <parameter name='dac_clk_freq_hz' value='100e6'/>
  </configuration>
  <configuration id='2'>
    <parameter name='dac_clk_freq_hz' value='200e6'/>
  </configuration>
  <configuration id='3'>
    <parameter name='dac_clk_freq_hz' value='100e6'/>
    <parameter name='DAC_OUTPUT_IS_LSB_OF_IN_PORT' value='true'/>
  </configuration>
  <configuration id='4'>
    <parameter name='ocpi_debug' value='true'/>
  </configuration>
</build>
